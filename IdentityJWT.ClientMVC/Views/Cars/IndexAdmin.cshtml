@model CarsListViewModel;
@{
    ViewBag.Title = "Index (Admin)";
}
<div>
    <table>
        <thead>
            <tr>
                <th> Brand</th>
                <th> Model</th>
                <th> Speed</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model.Cars)
            {
                <tr>
                    <td>
                        @car.Brand
                    </td>
                    <td>
                        @car.Model
                    </td>
                    <td>
                        @car.MaxSpeed
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

